{
  "name": "@sanity/ui",
  "version": "1.0.14",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "module": "./dist/index.esm.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "node": {
        "import": "./dist/index.cjs.mjs",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s clean build:pkg check:pkg",
    "build:pkg": "pkg build --strict",
    "check:pkg": "pkg --strict",
    "check:types": "tsc",
    "clean": "rimraf .workshop dist",
    "commit": "cz",
    "cypress:dev": "run-p dev cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-s workshop:dev",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint . --ext .cjs,.js,.jsx,.mjs,.ts,.tsx --quiet",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "release": "semantic-release",
    "test": "jest",
    "test:browser": "start-server-and-test 'run-s workshop:build workshop:start' http://localhost:1337 'run-s cypress:run'",
    "watch": "pkg watch --strict",
    "workshop:build": "node -r esbuild-register -r tsconfig-paths/register scripts/build",
    "workshop:dev": "node -r esbuild-register -r tsconfig-paths/register scripts/dev",
    "workshop:start": "http-server -a localhost -c-0 -p 1337 -s -P http://localhost:1337/index.html? dist"
  },
  "dependencies": {
    "@floating-ui/react-dom": "1.1.1",
    "@sanity/color": "^2.2.2",
    "@sanity/icons": "^2.2.2",
    "csstype": "^3.1.1",
    "framer-motion": "^8.4.2",
    "react-refractor": "^2.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@juggle/resize-observer": "^3.4.0",
    "@sanity/pkg-utils": "^2.2.1",
    "@sanity/semantic-release-preset": "^3.0.2",
    "@sanity/ui-workshop": "^1.2.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.5",
    "@types/jest-axe": "^3.5.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-is": "^17.0.3",
    "@types/refractor": "^3.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "commitizen": "^4.2.6",
    "cypress": "^12.3.0",
    "cypress-real-events": "^1.7.6",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.16.17",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "start-server-and-test": "^1.15.2",
    "styled-components": "^5.3.6",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^18",
    "styled-components": "^5.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/ui/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "components"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@7.21.0",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
